    fightBtn.Position = UDim2.new(0,0,0,50)
    fightBtn.BackgroundColor3 = Color3.fromRGB(70,0,0)
    fightBtn.Text = "Start AutoFight"
    fightBtn.Font = Enum.Font.GothamBold
