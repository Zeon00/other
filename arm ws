	while _G.loop7 == true do
    -- Equip Dumbells
    toolService:FireServer("PlayfulMines", "Dumbells", "PlayfulMines12")
    task.wait(60) -- wait 1 minute

    -- Equip Grips
    toolService:FireServer("PlayfulMines", "Grips", "PlayfulMines12")
    task.wait(60) -- wait 1 minute
