-- Fungsi rekursif PlayfulMines
local function getAllEggs(folder)
    local eggs = {}
    for _, child in ipairs(folder:GetChildren()) do
        if child:IsA("Folder") then
            local subEggs = getAllEggs(child)
            for _, e in ipairs(subEggs) do table.insert(eggs,e) end
        elseif child:IsA("BasePart") or child:IsA("Model") then
            table.insert(eggs, child)
        end
    end
    return eggs
end

-- Popup Egg
local function createEggPopup(eggName)
    local Popup = Instance.new("Frame")
    Popup.Size = UDim2.new(0,250,0,150)
    Popup.Position = UDim2.new(0.5,-125,0.5,-75)
    Popup.BackgroundColor3 = Color3.fromRGB(28,28,28)
    Popup.BorderSizePixel = 0
    Popup.Parent = ScreenGui
    local PCorner = Instance.new("UICorner", Popup)
    PCorner.CornerRadius = UDim.new(0,10)

    local TitleBarP = Instance.new("Frame")
    TitleBarP.Size = UDim2.new(1,0,0,28)
    TitleBarP.BackgroundColor3 = Color3.fromRGB(18,18,18)
    TitleBarP.Parent = Popup
    local TBCorner = Instance.new("UICorner", TitleBarP)
    TBCorner.CornerRadius = UDim.new(0,10)

    local TitleLabelP = Instance.new("TextLabel")
    TitleLabelP.Size = UDim2.new(1,-50,1,0)
    TitleLabelP.Position = UDim2.new(0,5,0,0)
    TitleLabelP.BackgroundTransparency = 1
    TitleLabelP.Text = "ðŸ¥š "..eggName
    TitleLabelP.TextColor3 = Color3.fromRGB(255,255,255)
    TitleLabelP.Font = Enum.Font.GothamSemibold
    TitleLabelP.TextScaled = true
    TitleLabelP.TextXAlignment = Enum.TextXAlignment.Left
    TitleLabelP.Parent = TitleBarP

    local CloseBtnP = Instance.new("TextButton")
    CloseBtnP.Size = UDim2.new(0,25,1,0)
    CloseBtnP.Position = UDim2.new(1,-25,0,0)
    CloseBtnP.Text = "X"
    CloseBtnP.TextColor3 = Color3.fromRGB(255,255,255)
    CloseBtnP.TextScaled = true
    CloseBtnP.Font = Enum.Font.GothamBold
    CloseBtnP.BackgroundColor3 = Color3.fromRGB(200,50,50)
    CloseBtnP.Parent = TitleBarP
    CloseBtnP.MouseButton1Click:Connect(function() Popup:Destroy() end)

    local ToggleBtn = Instance.new("TextButton")
    ToggleBtn.Size = UDim2.new(0.8,0,0.5,0)
    ToggleBtn.Position = UDim2.new(0.1,0,0.25,0)
    ToggleBtn.BackgroundColor3 = Color3.fromRGB(50,200,50)
    ToggleBtn.Text = "â–¶ Start"
    ToggleBtn.TextColor3 = Color3.fromRGB(255,255,255)
    ToggleBtn.Font = Enum.Font.GothamBold
    ToggleBtn.TextScaled = true
    ToggleBtn.Parent = Popup

    local running = false
    ToggleBtn.MouseButton1Click:Connect(function()
        running = not running
        if running then
            ToggleBtn.Text = "â–  Stop"
            ToggleBtn.BackgroundColor3 = Color3.fromRGB(200,50,50)
            task.spawn(function()
                while running do
                    pcall(EggService.InvokeServer, EggService, eggName, true, false)
                    task.wait(1)
                end
            end)
        else
            ToggleBtn.Text = "â–¶ Start"
            ToggleBtn.BackgroundColor3 = Color3.fromRGB(50,200,50)
        end
    end)

    -- Dragging Popup
    do
        local dragging = false
        local dragInput, dragStart, startPos

        TitleBarP.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                dragStart = input.Position
                startPos = Popup.Position
                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then dragging = false end
                end)
            end
        end)

        TitleBarP.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement then dragInput = input end
        end)

        UserInputService.InputChanged:Connect(function(input)
            if input == dragInput and dragging then
                local delta = input.Position - dragStart
                Popup.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
                                           startPos.Y.Scale, startPos.Y.Offset + delta.Y)
            end
        end)
    end
end

-- Hover effect helper
local function addHoverEffect(button, color1, color2)
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = color2
    end)
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = color1
    end)
end

-- Refresh Zones
local function refreshZones(filterText)
    ZoneList:ClearAllChildren()
    local zones = Workspace:WaitForChild("Zones"):GetChildren()
    table.sort(zones, function(a,b) return a.Name:lower() < b.Name:lower() end)
    local y = 0
    for i, zone in ipairs(zones) do
        if filterText == "" or zone.Name:lower():find(filterText:lower()) then
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(1,-10,0,32)
            btn.Position = UDim2.new(0,5,0,y)
            btn.BackgroundColor3 = Color3.fromRGB(70,70,70)
            btn.Text = i..". "..zone.Name
            btn.TextColor3 = Color3.fromRGB(255,255,255)
            btn.Font = Enum.Font.GothamSemibold
            btn.TextScaled = true
            btn.Parent = ZoneList
            addHoverEffect(btn, Color3.fromRGB(70,70,70), Color3.fromRGB(100,100,100))

            btn.MouseButton1Click:Connect(function()
                EggList:ClearAllChildren()
                local eggsFolder = zone:FindFirstChild("Interactables") and zone.Interactables:FindFirstChild("Eggs")
                if eggsFolder then
                    local eggs = {}
                    if zone.Name == "PlayfulMines" then
                        eggs = getAllEggs(eggsFolder)
                    else
                        eggs = eggsFolder:GetChildren()
                    end
                    local ey = 0
                    for _, egg in ipairs(eggs) do
                        local ebtn = Instance.new("TextButton")
                        ebtn.Size = UDim2.new(1,-10,0,28)
                        ebtn.Position = UDim2.new(0,5,0,ey)
                        ebtn.BackgroundColor3 = Color3.fromRGB(90,90,90)
                        ebtn.Text = egg.Name
                        ebtn.TextColor3 = Color3.fromRGB(255,255,255)
                        ebtn.Font = Enum.Font.GothamSemibold
                        ebtn.TextScaled = true
                        ebtn.Parent = EggList
                        addHoverEffect(ebtn, Color3.fromRGB(90,90,90), Color3.fromRGB(120,120,120))

                        ebtn.MouseButton1Click:Connect(function()
                            createEggPopup(egg.Name)
                        end)
                        ey = ey + 32
                    end
                    EggList.CanvasSize = UDim2.new(0,0,0,ey)
                end
            end)
            y = y + 36
        end
    end
    ZoneList.CanvasSize = UDim2.new(0,0,0,y)
end

-- Search listener
SearchBar:GetPropertyChangedSignal("Text"):Connect(function()
    refreshZones(SearchBar.Text)
end)

-- Initial load
refreshZones("")
 end})
GUI:CreateButton({
    parent = main, 
    text = "Auto Spinner Bot", 
    callback = function()
-- Services
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Knit Spinner Service
local SpinnerService = ReplicatedStorage:WaitForChild("Packages"):WaitForChild("Knit")
    :WaitForChild("Services"):WaitForChild("SpinnerService")

local SpinInfo = require(ReplicatedStorage.Data.SpinInfo)

-- GUI Setup
local ScreenGui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
ScreenGui.ResetOnSpawn = false

-- Main Frame (Compact)
