--// Remote reference
local SpinRemote = ReplicatedStorage
    :WaitForChild("Packages")
    :WaitForChild("Knit")
    :WaitForChild("Services")
    :WaitForChild("SpinnerService")
    :WaitForChild("RF")
    :WaitForChild("Spin")

-- Function to spin
local function doSpin(name, amount)
    if name ~= "" and amount ~= "" then
        local args = { name, amount }
        local success, result = pcall(function()
            return SpinRemote:InvokeServer(unpack(args))
        end)
        if success then
            print("Spun:", name, amount, "Result:", result)
        else
            warn("âŒ Failed to spin:", name, amount)
        end
    end
end

-- Button toggle logic
Toggle.MouseButton1Click:Connect(function()
    if loopRunning then
        loopRunning = false
        Toggle.Text = "â–¶ Start Auto Spin"
        Toggle.BackgroundColor3 = Color3.fromRGB(60, 100, 60)
    else
        spinName = NameBox.Text
        spinAmount = AmountBox.Text
        if spinName == "" or spinAmount == "" then
            warn("âš ï¸ Please enter spin name & amount first!")
            return
        end
        loopRunning = true
        Toggle.Text = "â–  Stop Auto Spin"
        Toggle.BackgroundColor3 = Color3.fromRGB(120, 40, 40)

        -- Start loop
        task.spawn(function()
            while loopRunning do
                doSpin(spinName, spinAmount)
                task.wait(0.2) -- small delay
            end
        end)
    end
end)

-- Minimize / Restore
MinBtn.MouseButton1Click:Connect(function()
    Frame.Visible = false
    MiniIcon.Visible = true
end)

MiniIcon.MouseButton1Click:Connect(function()
    Frame.Visible = true
    MiniIcon.Visible = false
end)

-- Close GUI
CloseBtn.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)
end})
