    autoFarmEnabled = not autoFarmEnabled
    if autoFarmEnabled then
        FarmButton.Text = "AutoFarm: ON"
        FarmButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        status = "Idle..."
    else
        FarmButton.Text = "AutoFarm: OFF"
