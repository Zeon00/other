--// Remote reference
local EggRemote = ReplicatedStorage
    :WaitForChild("Packages")
    :WaitForChild("Knit")
    :WaitForChild("Services")
    :WaitForChild("EggService")
    :WaitForChild("RF")
    :WaitForChild("purchaseEgg")

local ItemRemote = ReplicatedStorage
    :WaitForChild("Packages")
    :WaitForChild("Knit")
    :WaitForChild("Services")
    :WaitForChild("ItemService")
    :WaitForChild("RF")
    :WaitForChild("UseItem")

-- Auto-use potions before loop
local function usePotions()
    local potions = {"Golden Potion","Luck Potion","Void Potion"}
    for _,p in ipairs(potions) do
        local args = {p,"1"}
        pcall(function()
            ItemRemote:InvokeServer(unpack(args))
        end)
    end
end

-- Function to buy egg
local function openEgg(name)
    if name and name ~= "" then
        local args = {name,[5] = true,[4] = false}
        pcall(function()
            EggRemote:InvokeServer(unpack(args))
        end)
    end
end

-- Button toggle logic
Toggle.MouseButton1Click:Connect(function()
    if loopRunning then
        loopRunning = false
        Toggle.Text = "â–¶ Start Loop"
        Toggle.BackgroundColor3 = Color3.fromRGB(60, 100, 60)
    else
        if EggBox.Text == "" then
            warn("âš ï¸ Enter an Egg Name first!")
            return
        end
        loopRunning = true
        Toggle.Text = "â–  Stop Loop"
        Toggle.BackgroundColor3 = Color3.fromRGB(120, 40, 40)

        -- Use potions before loop
        usePotions()

        -- Start loop
        task.spawn(function()
            while loopRunning do
                openEgg(EggBox.Text)
                task.wait() -- âš¡ spam fast, adjust if needed
            end
        end)
    end
end)

-- Minimize / Restore
MinBtn.MouseButton1Click:Connect(function()
    Frame.Visible = false
    MiniIcon.Visible = true
end)

MiniIcon.MouseButton1Click:Connect(function()
    Frame.Visible = true
    MiniIcon.Visible = false
end)

-- Close GUI
CloseBtn.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

    end
})
