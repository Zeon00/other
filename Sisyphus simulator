local tgls = serv:Channel("Player") 
tgls:Toggle("Auto Click",false, function(spin)
if spin then
        _G.spin1 = true
	while _G.spin1 == true do
		task.wait()
	local args = {"1_4"}
game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Tool"):WaitForChild("[C-S]TryBuyAndUseTool"):FireServer(unpack(args))
local args = {game:GetService("Players").LocalPlayer.Character:WaitForChild("1_4")}
game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerClick"):FireServer(unpack(args))
       end
		else
		_G.spin1 = false
           end
		   print(spin)
end)

tgls:Toggle("Auto Push Easter",false, function(Easter)
if Easter then
        _G.Easter1 = true
	while _G.Easter1 == true do
		task.wait()
game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerTryBall"):FireServer(-10)
game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerEnd"):FireServer(true,1)
       end
		else
		_G.Easter1 = false
           end
		   print(Easter)
end)
